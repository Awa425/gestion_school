{% extends 'base.html.twig' %}

{# {% block title %}Hello EtudiantController!
{% endblock %} #}

{% block body %}
	<style>
		.example-wrapper {
			margin: 1em auto;
			max-width: 800px;
			width: 95%;
			font: 18px / 1.5 sans-serif;
		}
		.example-wrapper code {
			background: #F5F5F5;
			padding: 2px 6px;
		}
	</style>

	<div class="example-wrapper">
		<table class="table table-hover">
			<thead>
				<tr class='text-center'>
					<th scope="col">Nom Complet</th>
					<th scope="col">Adresse</th>
					<th scope="col">Sexe</th>
					<th scope="col">Matricule</th>
					<th scope="col">Attache</th>
					<th scope="col">Options</th>
				</tr>
			</thead>
			<tbody>
				{% for ins in insrits %}
					<tr>
						<td>{{ ins.etudiant.nomComplet}}</td>
						<td>{{ ins.etudiant.adresse}}</td>
						<td>{{ ins.etudiant.sexe}}</td>
						<td>{{ ins.etudiant.matricule}}</td>
						<td>{{ ins.ac.nomComplet}}</td>
						{# <td>{{ ins.anneescolaire_id.libelle}}</td> #}
						{# <td>{{ ins.classes.libelle}}</td> #}
						{# <td>{{ ins.nomComplet}}</td> #}
							<td class="text-center"> <a href="">Archiver</a>
							<a href="">Modifier</a>
							<a href="">Detail</a>
						</td>
					</tr>
				{% endfor %}
			</tbody>
		</table>

	</div>

	<div class='container pagination'>
		<span class=''>
			{{ knp_pagination_render(insrits) }}
		</span>
	</div>
{% endblock %}
